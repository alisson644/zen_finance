<div id="<%= dom_id transaction %>">
  <p>
    <strong>Kind:</strong>
    <%= transaction.kind %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= transaction.description %>
  </p>

  <p>
    <strong>Value:</strong>
    <%= transaction.value %>
  </p>

  <p>
    <strong>User:</strong>
    <%= transaction.user_id %>
  </p>

  <p>
    <strong>Source:</strong>
    <%= transaction.source_id %>
  </p>

  <p>
    <strong>Date transaction:</strong>
    <%= transaction.date_transaction %>
  </p>

</div>
